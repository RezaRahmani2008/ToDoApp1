<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/correntToDos">
    <fragment
        android:id="@+id/correntToDos"
        android:name="com.example.todoapp.CorrentToDos"
        android:label="fragment_corrent_to_dos"
        tools:layout="@layout/fragment_corrent_to_dos" >
        <action
            android:id="@+id/action_correntToDos_to_addTask"
            app:destination="@id/addTask" />
    </fragment>
    <fragment
        android:id="@+id/addTask"
        android:name="com.example.todoapp.AddTask"
        android:label="fragment_add_task"
        tools:layout="@layout/fragment_add_task" >
        <action
            android:id="@+id/action_addTask_to_correntToDos"
            app:destination="@id/correntToDos" />
    </fragment>
</navigation>